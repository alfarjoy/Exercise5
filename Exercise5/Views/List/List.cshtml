@model IEnumerable<Exercise5.Models.List>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "List of Registered";
}
<h2>Users</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>User Id</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Address</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Birthdate</th>
            <th>Mobile Number</th>
            <th>Email Address</th>
            <th colspan="2">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.Lname</td>
                <td>@user.Fname</td>
                <td>@user.Mname</td>
                <td>@user.Address</td>
                <td>@user.Age</td>
                <td>@user.Gender</td>
                <td>@user.Bday</td>
                <td>@user.MobileNumber</td>
                <td>@user.EmailAddress</td>
                <td><a asp-action="edit" asp-route-id="@user.UserId" class="text-info">Edit</a></td>
                <td><a asp-action="delete" asp-route-id="@user.UserId" class="text-danger">Delete</a></td>
            </tr>
        }
    </tbody>
</table>